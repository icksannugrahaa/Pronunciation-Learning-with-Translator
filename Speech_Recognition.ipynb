        "  audio_file = sorted(np.array(tf.io.gfile.glob(str(newer_path) + '/*/*.wav')))\n",
        "  text_file = sorted(np.array(tf.io.gfile.glob(str(newer_path) + '/*/*.txt')))\n",
        "  list_ds = tf.data.Dataset.from_tensor_slices((audio_file, text_file)).cache()\n",
